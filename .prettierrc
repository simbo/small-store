{
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": ["<BUILTIN_MODULES>", "", "<THIRD_PARTY_MODULES>", "", "^src/", "", "^\\.\\.(/|$)", "", "^\\.(/|$)"],
  "importOrderParserPlugins": ["typescript", "decorators-legacy"],
  "arrowParens": "avoid",
  "importOrderTypeScriptVersion": "5.2.2",
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "none",
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "printWidth": 80,
        "proseWrap": "always"
      }
    }
  ]
}
